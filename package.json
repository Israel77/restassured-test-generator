{
    "name": "restassured-test-generator",
    "version": "0.0.11-rc.1",
    "description": "Generates RestAssured assertions based on a given JSON input",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "scripts": {
        "dev": "tsc --watch --sourceMap",
        "lint": "eslint .",
        "clean": "rm -r ./dist ./coverage ./coverage-ts && echo 'dist and coverage directories cleaned' || 'dist directory is already clean'",
        "clean:pack": "rm -r ./package; rm restassured-test-generator*.tgz",
        "test": "mocha",
        "coverage": "c8 npm run test",
        "type-coverage": "typescript-coverage-report",
        "build": "npm run clean; tsc && echo 'Successful build'"
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@eslint/js": "^9.7.0",
        "@types/chai": "^4.3.16",
        "@types/eslint__js": "^8.42.3",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.14.2",
        "c8": "^10.1.2",
        "chai": "^5.1.1",
        "eslint": "^8.57.0",
        "mocha": "^10.4.0",
        "tsx": "^4.15.5",
        "typescript": "^5.5.3",
        "typescript-coverage-report": "^1.0.0",
        "typescript-eslint": "^7.16.1"
    },
    "type": "module",
    "files": [
        "dist/**/*",
        "!dist/test",
        "types"
    ]
}